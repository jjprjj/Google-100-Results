{
  "manifest_version": 3,
  "name": "Google 100 Results",
  "version": "1.2",
  "description": "Load 100 Google search results on one page",
  "permissions": ["activeTab", "storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Google 100 Results",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*", "*://google.com/search*"],
      "js": ["script.js"],
      "run_at": "document_idle"
    }
  ]
}